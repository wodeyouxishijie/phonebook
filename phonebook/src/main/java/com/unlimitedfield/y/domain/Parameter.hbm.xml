<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.unlimitedfield.y.domain">
	<class name="Parameter" table="tbl_parameter" abstract="true">
		<id name="id" column="`id`" type="long">
			<generator class="native"></generator>
		</id>
		<!-- 用于鉴别class -->
		<discriminator column="`class`" type="string" length="255" />

		<property name="key" column="`key`" type="string" length="255" />
		<property name="value" column="`value`" type="string" length="255" />
		<property name="remark" column="`remark`" type="string" length="255" />
	</class>
</hibernate-mapping>
